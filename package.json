{
  "name": "complianceai-saas",
  "version": "1.0.0",
  "description": "AI-powered legal compliance platform for Indian startups",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "docker:build": "docker build -t complianceai .",
    "docker:run": "docker run -p 3000:3000 complianceai",
    "docker:dev": "docker-compose up -d",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "terraform:init": "cd terraform && terraform init",
    "terraform:plan": "cd terraform && terraform plan",
    "terraform:apply": "cd terraform && terraform apply",
    "deploy:staging": "aws ecs update-service --cluster staging --service complianceai",
    "deploy:production": "aws ecs update-service --cluster production --service complianceai",
    "backup:db": "./scripts/backup.sh",
    "restore:db": "./scripts/restore.sh",
    "security:audit": "npm audit && snyk test",
    "performance:test": "lighthouse-ci autorun",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "prisma": "^5.5.0",
    "@prisma/client": "^5.5.0",
    "bcryptjs": "^2.4.3",
    "@types/bcryptjs": "^2.4.4",
    "jsonwebtoken": "^9.0.2",
    "@types/jsonwebtoken": "^9.0.3",
    "zod": "^3.22.0",
    "openai": "^4.14.0",
    "razorpay": "^2.9.2",
    "@types/razorpay": "^2.0.2",
    "resend": "^2.0.0",
    "puppeteer": "^21.4.0",
    "handlebars": "^4.7.8",
    "react-hook-form": "^7.47.0",
    "@hookform/resolvers": "^3.3.0",
    "@tanstack/react-query": "^5.0.0",
    "lucide-react": "^0.288.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-calendar": "^1.0.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "react-day-picker": "^8.9.0",
    "date-fns": "^2.30.0",
    "zustand": "^4.4.1",
    "winston": "^3.10.0",
    "winston-cloudwatch": "^6.0.0",
    "bull": "^4.11.3",
    "ioredis": "^5.3.2",
    "express-rate-limit": "^6.10.0",
    "@sentry/nextjs": "^7.70.0",
    "multer": "^1.4.5-lts.1",
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/client-ses": "^3.400.0",
    "next-pwa": "^5.6.0"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "eslint-config-next": "^14.0.0",
    "prettier": "^3.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "tsx": "^3.14.0",
    "jest": "^29.7.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/user-event": "^14.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-mocks-http": "^1.13.0",
    "@playwright/test": "^1.40.0",
    "lighthouse-ci": "^0.12.0",
    "snyk": "^1.1200.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
